{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "pacific-1": {
      "type": "array",
      "items": { "$ref": "#/definitions/token" }
    },
    "atlantic-2": {
      "type": "array",
      "items": { "$ref": "#/definitions/token" }
    },
    "arctic-1": {
      "type": "array",
      "items": { "$ref": "#/definitions/token" }
    },
    "sei-devnet-3": {
      "type": "array",
      "items": { "$ref": "#/definitions/token" }
    }
  },
  "additionalProperties": false,
  "definitions": {
    "token": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "denom_units": {
          "type": "array",
          "items": { "$ref": "#/definitions/denom_unit" }
        },
        "base": {
          "type": "string"
        },
        "display": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        },
        "images": {
          "type": "object",
          "properties": {
            "png": {
              "type": "string",
              "format": "uri-reference"
            },
            "svg": {
              "type": "string",
              "format": "uri-reference"
            }
          }
        },
        "coingecko_id": {
          "type": "string",
          "description": "[OPTIONAL] The coingecko id to fetch asset data from coingecko v3 api. See https://api.coingecko.com/api/v3/coins/list"
        },
        "type_asset": {
          "type": "string",
          "enum": ["sdk.coin", "cw20", "erc20", "ics20"],
          "default": "sdk.coin",
          "description": "[OPTIONAL] The potential options for type of asset. By default, assumes sdk.coin"
        }
      },
      "required": [
        "description",
        "denom_units",
        "base",
        "display",
        "name",
        "symbol",
        "images"
      ],
      "additionalProperties": false
    },
    "denom_unit": {
      "type": "object",
      "properties": {
        "denom": {
          "type": "string"
        },
        "exponent": {
          "type": "integer",
          "minimum": 0
        }
      },
      "required": ["denom", "exponent"],
      "additionalProperties": false
    }
  }
}
